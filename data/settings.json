{
	"username": "pk",
	"title": "New Tab",
	"theme": {
		"name": "pk-custom",
		"backgroundColor": "#000000",
		"windowColor": "#09090B",
		"textColor": "#DEE3F1",
		"black": "#16161e",
		"red": "#FCA5A5",
		"green": "#86EEAC",
		"yellow": "#FDDF47",
		"blue": "#93C5FD",
		"magenta": "#C4B5FD",
		"cyan": "#67E7F9",
		"white": "#e2e2e2",
		"gray": "#97989d"
	},
	"wallpaper": {
		"url": "assets/bg.jpg",
		"easing": "ease-in-out",
		"fadeIn": true,
		"blur": true
	},
	"terminal": {
		"fixedHeight": false,
		"windowGlow": false,
		"windowGlowColor": "#291f3325",
		"textGlow": false,
		"opacity": 1,
		"blur": 16
	},
	"prompt": {
		"ctrlC": true,
		"placeholder": "search the web...",
		"placeholderColor": "gray",
		"userColor": "green",
		"atColor": "gray",
		"hostColor": "magenta",
		"promptColor": "magenta",
		"promptSymbol": "‚ùØ",
		"caretColor": "blue",
		"selectionBg": "yellow",
		"selectionFg": "black"
	},
	"fetch": {
		"timeFormat": "HH:mm",
		"dateFormat": "DD/MM/YYYY",
		"titleColor": "yellow",
		"image": "",
		"data": [
			"Time: {time}",
			"Date: {date}",
			"{seperator}",
			"OS: {osName} {osVersion}",
			"Theme: {theme}",
			"{seperator}",
			"Browser: {browser} {browserVersion}",
			"Engine: {engineName}",
			"{seperator}",
			"{colors}"
		]
	},
	"urlLaunch": {
		"target": "_self",
		"hoverColor": "magenta"
	},
	"search": {
		"default": "https://google.com/search?q={}",
		"target": "_self",
		"shortcutRegex": "(!?[A-Za-z0-9]+) (.*)",
		"shortcuts": [
			{
				"alias": "d",
				"name": "DuckDuckGo Search",
				"url": "https://duckduckgo.com/?q={}"
			},
			{
				"alias": "gh",
				"name": "Github Search",
				"url": "https://github.com/search?q={}"
			},
			{
				"alias": "w",
				"name": "Wikipedia Search",
				"url": "https://en.wikipedia.org/wiki/{}"
			},
			{
				"alias": "yt",
				"name": "YouTube Search",
				"url": "https://www.youtube.com/results?search_query={}"
			}
		]
	},
